{
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter native dev",
    "dev:native": "pnpm --filter native dev",
    "dev:mobile": "pnpm --filter native dev",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "lint": "turbo run lint",
    "lint:web": "pnpm --filter web lint",
    "lint:native": "pnpm --filter native lint",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "android": "cd apps/native && npx expo run:android",
    "ios": "cd apps/native && npx expo run:ios",
    "prebuild": "cd apps/native && npx expo prebuild",
    "prebuild:clean": "cd apps/native && npx expo prebuild --clean",
    "start": "cd apps/native && npx expo start",
    "start:clear": "cd apps/native && npx expo start --clear"
  },
  "devDependencies": {
    "@babel/helper-module-imports": "^7.27.1",
    "@babel/template": "^7.27.2",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "apps/web/**/*.{js,jsx,ts,tsx}": [
      "pnpm --filter web lint",
      "prettier --write"
    ],
    "apps/native/**/*.{js,jsx,ts,tsx}": [
      "pnpm --filter native lint",
      "prettier --write"
    ],
    "packages/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "name": "with-react-native-web",
  "packageManager": "pnpm@10.12.4",
  "version": "1.0.0",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.56.0",
    "expo": "~52.0.47",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-web-maps": "^0.3.0",
    "react-redux": "^9.2.0"
  }
}
